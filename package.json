{
  "name": "apd",
  "version": "3.0.0",
  "contributors": [
    "The eTools Team"
  ],
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git@github.com:unicef/etools-action-points"
  },
  "bugs": {
    "url": "https://github.com/unicef/etools-action-points/issues"
  },
  "description": "Action Points Dashboard for UNICEF eTools",
  "scripts": {
    "tsToJs": "tsc --skipLibCheck || true",
    "tsEslint": "node_modules/eslint/bin/eslint.js 'src_ts/**' --fix",
    "start": "tsc --watch --preserveWatchOutput & polymer serve -H 0.0.0.0 -p 8080",
    "build": "export NODE_OPTIONS=--max_old_space_size=4096 && npm run tsToJs && npm run build:static",
    "build:prpl-server": "polymer build --auto-base-path && gulp prpl-server",
    "build:static": "polymer build",
    "serve:prpl-server": "prpl-server --root server/build",
    "serve:static": "polymer serve --port 5000 build/es6-bundled",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "tsc && mocha test/integration --timeout=10000",
    "test:unit": "tsc && polymer test --module-resolution=node --npm",
    "test:sauce": "tsc && polymer test --module-resolution=node --npm -s 'windows 10/microsoftedge@17' -s 'macos 10.13/safari@11'",
    "test:regenerate_screenshots": "tsc && mocha test/integration/screenshots-baseline/regenerate.js --timeout=15000",
    "lint:eslint": "eslint --ext .ts . --ignore-path .gitignore",
    "format:eslint": "eslint --ext .ts . --fix --ignore-path .gitignore",
    "lint:prettier": "prettier \"**/*.ts\" --check --ignore-path .gitignore",
    "format:prettier": "prettier \"**/*.ts\" --write --ignore-path .gitignore",
    "lint": "npm run lint:eslint",
    "format": "npm run format:eslint"
  },
  "dependencies": {
    "@polymer/app-layout": "^3.1.0",
    "@polymer/app-route": "^3.0.2",
    "@polymer/decorators": "^3.0.0",
    "@polymer/iron-collapse": "^3.0.1",
    "@polymer/iron-flex-layout": "^3.0.1",
    "@polymer/iron-icons": "^3.0.1",
    "@polymer/iron-meta": "^3.0.1",
    "@polymer/iron-pages": "^3.0.1",
    "@polymer/iron-selector": "^3.0.1",
    "@polymer/neon-animation": "^3.0.1",
    "@polymer/paper-button": "^3.0.1",
    "@polymer/paper-card": "^3.0.1",
    "@polymer/paper-checkbox": "^3.1.0",
    "@polymer/paper-dropdown-menu": "^3.2.0",
    "@polymer/paper-icon-button": "^3.0.2",
    "@polymer/paper-input": "^3.2.1",
    "@polymer/paper-item": "^3.0.1",
    "@polymer/paper-listbox": "^3.0.1",
    "@polymer/paper-material": "^3.0.1",
    "@polymer/paper-ripple": "^3.0.2",
    "@polymer/paper-styles": "^3.0.1",
    "@polymer/paper-toggle-button": "^3.0.1",
    "@polymer/paper-tooltip": "^3.0.1",
    "@polymer/polymer": "^3.4.1",
    "@unicef-polymer/etools-ajax": "^4.3.4",
    "@unicef-polymer/etools-app-selector": "^3.1.8",
    "@unicef-polymer/etools-behaviors": "^3.0.5",
    "@unicef-polymer/etools-content-panel": "5.0.0",
    "@unicef-polymer/etools-data-table": "^6.0.0",
    "@unicef-polymer/etools-date-time": "^3.0.1",
    "@unicef-polymer/etools-dialog": "^6.0.1",
    "@unicef-polymer/etools-dropdown": "^3.7.9",
    "@unicef-polymer/etools-loading": "^5.0.1",
    "@unicef-polymer/etools-modules-common": "^1.2.3",
    "@unicef-polymer/etools-piwik-analytics": "^3.0.2",
    "@unicef-polymer/etools-profile-dropdown": "^7.0.1",
    "@webcomponents/shadycss": "^1.11.0",
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "dayjs": "^1.10.7",
    "dexie": "^3.2.0",
    "linkifyjs": "^2.1.8",
    "pwa-helpers": "^0.9.0",
    "sinon": "^9.0.0",
    "stream-combiner2": "^1.1.1",
    "web-animations-js": "^2.3.2"
  },
  "devDependencies": {
    "@polymer/test-fixture": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "@typescript-eslint/typescript-estree": "^5.6.0",
    "axe-core": "^3.5.5",
    "browserify": "^16.5.2",
    "chai": "^4.3.0",
    "del": "^3.0.0",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "gulp": "^4.0.2",
    "mocha": "^5.2.0",
    "pixelmatch": "^4.0.2",
    "polymer-cli": "^1.9.11",
    "polyserve": "^0.27.14",
    "prettier": "^2.5.1",
    "prpl-server": "^1.4.2",
    "puppeteer": "^1.5.0",
    "typescript": "^4.5.3",
    "wct-mocha": "^1.1.0",
    "ua-parser-js": ">=0.7.31",
    "cached-path-relative": ">=1.1.0",
    "follow-redirects": ">=1.14.9",
    "urijs": ">=1.19.9",
    "ajv": ">=6.12.6"
  }
}
