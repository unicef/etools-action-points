    "etools-content-panel": "git+https://github.com/unicef-polymer/etools-content-panel.git#update-to-polymer3",
    "etools-data-table": "git+https://github.com/unicef-polymer/etools-data-table.git#upgrade-to-polymer3",
    "etools-dialog": "git+https://github.com/unicef-polymer/etools-dialog.git#upgrade-to-polymer3",
