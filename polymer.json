{
  "entrypoint": "index.html",
  "shell": "src/app-shell.js",
  "fragments": [
    "src/elements/pages/not-found-page-view.js",
    "src/elements/pages/action-points/detail/action-point-comments.js",
    "src/elements/pages/action-points/detail/action-point-details.js",
    "src/elements/pages/action-points/detail/action-points-history.js",
    "src/elements/pages/action-points/detail/action-points-item.js",
    "src/elements/pages/action-points/action-points-list.js",
    "src/elements/pages/action-points/action-points-new.js",
    "src/elements/pages/action-points/action-points-page-main.js",
    "src/elements/pages/action-points/detail/open-add-comments.js",
    "src/elements/pages/action-points/detail/open-view-history.js"
  ],
  "sources": ["src/**/*", "images/**/*", "package.json"],
  "extraDependencies": [
    "manifest.json",
    "version.json",
    "node_modules/@webcomponents/webcomponentsjs/**",
    "node_modules/web-animations-js/web-animations-next-lite.min.js",
    "node_modules/dayjs/dayjs.min.js",
    "node_modules/dayjs/plugin/utc.js",
    "node_modules/linkifyjs/**"
  ],
  "lint": {
    "rules": ["polymer-3"],
    "ignoreWarnings": ["could-not-resolve-reference"]
  },
  "builds": [
    {
      "name": "esm-bundled",
      "browserCapabilities": ["es2015", "modules"],
      "js": {
        "minify": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": true
    },
    {
      "name": "es6-bundled",
      "preset": "es6-bundled",
      "addServiceWorker": true
    }
  ],
  "moduleResolution": "node",
  "npm": true
}
