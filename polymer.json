{
  "entrypoint": "index.html",
  "shell": "./src/app-shell.js",
  "sources": [
    "images/**/*",
    "src/**/*",
    "elements/**/*"
  ],
  "fragments": [
    "src/elements/pages/**"
  ],
  "extraDependencies": [
    "manifest.json",
    "node_modules/@webcomponents/webcomponentsjs/**",
    "node_modules/web-animations-js/*.js"
  ],
  "lint": {
    "rules": ["polymer-3"]
  },
  "builds": [
    {
      "name": "esm-bundled",
      "browserCapabilities": [
        "es2015",
        "modules",
        "serviceworker"
      ],
      "js": {
        "minify": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": false,
      "addServiceWorker": true
    },
    {
      "name": "es6-bundled",
      "preset": "es6-bundled",
      "browserCapabilities": [
        "es2015"
      ],
      "js": {
        "compile": true,
        "minify": true,
        "transformModulesToAmd": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": false,
      "addServiceWorker": true
    },
    {
      "name": "es5-bundled",
      "preset": "es5-bundled",
      "js": {
        "compile": true,
        "minify": true,
        "transformModulesToAmd": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": false,
      "addServiceWorker": true
    }
  ],
  "moduleResolution": "node",
  "npm": true
}
