{
  "entrypoint": "index.html",
  "shell": "src/app-shell.js",
  "fragments": [
    "src/elements/pages/not-found-page-view.js"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "package.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "node_modules/@webcomponents/webcomponentsjs/**",
    "node_modules/web-animations-js/**",
    "node_modules/linkifyjs/**"
  ],
  "lint": {
    "rules": ["polymer-3"],
    "ignoreWarnings": ["could-not-resolve-reference"]
  },
  "builds": [
    {
      "name": "esm-bundled",
      "browserCapabilities": [
        "es2015",
        "modules"
      ],
      "js": {
        "minify": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": true
    },
    {
      "name": "es6-bundled",
      "preset": "es6-bundled",
      "addServiceWorker": true
    },
    {
      "name": "es5-bundled",
      "preset": "es5-bundled",
      "addServiceWorker": true
    }
  ],
  "moduleResolution": "node",
  "npm": true
}
